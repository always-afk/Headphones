@model HeadphonesShop.PresentationWebMVC.Models.ViewModels.AddHeadphonesViewModel

<div class="container">
    <form class="form-group" method="post" asp-controller="Admin" asp-action="AddHeadphones" enctype="multipart/form-data">
        <div class="form-group">
            <label>Name</label>
            <input class="form-control w-25" name="headphonesViewModel.Headphones.Name" />
        </div>
        <div class="form-group">
            <label>MinFrequency</label>
            <input class="form-control w-25" type="number" min="0" max="999999" step="1" name="headphonesViewModel.Headphones.MinFrequency" />
        </div>
        <div class="form-group">
            <label>MaxFrequency</label>
            <input class="form-control w-25" type="number" min="0" max="999999" step="1" name="headphonesViewModel.Headphones.MaxFrequency" />
        </div>
        <div class="form-group">
            <label>Company</label>
            <select class="custom-select w-25" name="headphonesViewModel.Headphones.Company.Name">
                @foreach (var i in Model.Companies)
                {
                    <option value="@i.Name">@i.Name</option>
                }
            </select>
        </div>
        <div class="form-group">
            <label>Design</label>
            <select class="custom-select w-25" name="headphonesViewModel.Headphones.Design.Name">
                @foreach (var i in Model.Designs)
                {
                    <option value="@i.Name">@i.Name</option>
                }
            </select>
        </div>
        <div class="form-group">
            <label>Picture</label>
            <input type="file" name="headphonesViewModel.File" class="custom-file"/>
        </div>
        
        
        <input type="submit" value="Add" />
    </form>
</div>